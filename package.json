{
  "name": "be-client",
  "version": "0.1.0",
  "description": "Javascript client for #be",
  "main": "./lib/index.js",
  "author": "Christopher Gibson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HashtagBe/be-client.git"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf lib dist",
    "build": "npm run build:dist && npm run build:commonjs",
    "build:dist": "NODE_ENV=production webpack --progress --colors --config webpack.config.js",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "repl": "./repl",
    "lint": "eslint src/**/*.js"
  },
  "dependencies": {
    "axios": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.10.2",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.0",
    "webpack-notifier": "^1.3.0",
    "xmlhttprequest": "^1.8.0"
  }
}
